<div class="container">
  <h1>Clubs</h1>
  <div class="row">
    <div class="col-md-12 col-lg-12">
      <div class="row">
        {{# each clubs}}
          <div class="col-md-4 col-lg-4">
            <h2 style="text-align:center"><a href="{{clubUrl slug}}">{{{title}}}</a></h2>
            {{# if image}}
              <img src="{{{cloudinaryUrl image width=400 height=200 crop="crop"}}}" class="thumbnail">
            {{/if}}
          </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>
